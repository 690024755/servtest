<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="WxServiceLoginTest" parallel="classes" thread-count="5">
    <test verbose="2" preserve-order="true" name="WxServiceLoginTest">
        <classes>
            <class name="com.galaxyeye.websocket.test.galaxyeye.WxService.TestCase.WxServiceLoginTest">
                <methods>
                    <include name="WxServiceLoginTestByExistParameterStatisticsAndExistUser"></include>
                    <include name="WxServiceLoginTestByNotExistParameterStatisticsAndExistUser"></include>
                    <include name="WxServiceLoginTestByExistParameterStatisticsAndNotExistUser"></include>
                    <include name="WxServiceLoginTestByNotExistParameterStatisticsAndNotExistUser"></include>
                    <include name="WxServiceLoginTestByParameterMaintenanceSubscribe"></include>
                </methods>
            </class>
        </classes>
    </test>

    <listeners>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="com.galaxyeye.websocket.test.galaxyeye.CustomReport.CustomReport"/>
    </listeners>
</suite>